pipeline {
    agent any
    stages {
        stage('git') {
            steps {
                dir('/home/prasad/projects/jenkins/'){
                    echo "BRANCH NAME ==> ${BRANCH}"
                    sh "git status"
                    sh "git checkout ${BRANCH}"
                    sh "git pull"
                }
            }
        }
        stage {
            steps {
                dir('/home/prasad/projects/jenkins/codedesignlabs_node/backend') {
                    sh 'npm install'
                    sh 'tsc -p tsconfig.json'
                    sh 'node ./dist/index.js'
                }
            }
        }
    }
}